    "redux": "^4.0.1"
  },
  "devDependencies": {
    "@open-wc/building-rollup": "^2.0.1",
    "es-dev-server": "^1.14.2",
    "http-server": "^14.1.0",
    "rimraf": "^2.6.3",
    "rollup": "^1.6.0"
  }
}
